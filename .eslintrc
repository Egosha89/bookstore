{
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "airbnb"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "plugins": [
    "react",
    "jsx-a11y",
    "import",
    "no-unused-vars-rest"
  ],
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "no-unused-vars": 0,
    "indent": [
      2,
      2,
      {"SwitchCase": 1, "VariableDeclarator": 1}
    ],
    "react/prop-types": 0,
    "react/jsx-indent": [0, "tab"],
    "react/jsx-indent-props": [0, "tab"],
    "linebreak-style": 0,
    "quotes": [
      "error",
      "single"
    ],
    "quote-props": [
      "warn",
      "consistent",
      {
        "unnecessary": true
      }
    ],
    "semi": [
      "error",
      "always"
    ]
  }
}